#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jflower/devProjects/rife-pediatrics-svelte/rife-pediatrics/node_modules/.pnpm/svelte-check@3.6.0_postcss@8.4.31_svelte@5.0.0-next.7/node_modules/svelte-check/bin/node_modules:/Users/jflower/devProjects/rife-pediatrics-svelte/rife-pediatrics/node_modules/.pnpm/svelte-check@3.6.0_postcss@8.4.31_svelte@5.0.0-next.7/node_modules/svelte-check/node_modules:/Users/jflower/devProjects/rife-pediatrics-svelte/rife-pediatrics/node_modules/.pnpm/svelte-check@3.6.0_postcss@8.4.31_svelte@5.0.0-next.7/node_modules:/Users/jflower/devProjects/rife-pediatrics-svelte/rife-pediatrics/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jflower/devProjects/rife-pediatrics-svelte/rife-pediatrics/node_modules/.pnpm/svelte-check@3.6.0_postcss@8.4.31_svelte@5.0.0-next.7/node_modules/svelte-check/bin/node_modules:/Users/jflower/devProjects/rife-pediatrics-svelte/rife-pediatrics/node_modules/.pnpm/svelte-check@3.6.0_postcss@8.4.31_svelte@5.0.0-next.7/node_modules/svelte-check/node_modules:/Users/jflower/devProjects/rife-pediatrics-svelte/rife-pediatrics/node_modules/.pnpm/svelte-check@3.6.0_postcss@8.4.31_svelte@5.0.0-next.7/node_modules:/Users/jflower/devProjects/rife-pediatrics-svelte/rife-pediatrics/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svelte-check" "$@"
else
  exec node  "$basedir/../../bin/svelte-check" "$@"
fi
